{
  "name" : "zodream/web",
  "description": "ZoDream PHP Framework",
  "version" : "4.0",
  "homepage": "https://zodream.cn",
  "license": "MIT",
  "require" : {
    "php" : ">=8.0",
    "zodream/zodream" : "^5.0",
    "zodream/disk" : "^5.0",
    "zodream/gzo" : "^5.0",
    "zodream/helpers" : "^5.0",
    "zodream/http" : "^5.0",
    "zodream/thirdparty" : "^5.0",
    "zodream/database" : "^5.0",
    "zodream/html" : "^5.0",
    "zodream/route" : "^5.0",
    "zodream/template" : "^5.0",
    "zodream/image" : "^5.0",
    "zodream/validate": "^5.0",
    "zodream/debugger": "^5.0",
    "zodream/oauth" : "dev-master",
    "zodream/spider": "dev-master",
    "zodream/wechat" : "^4.0",

    "psr/http-message": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "psr/container": "^1.1",
    "psr/log": "^1.1",
    "monolog/monolog": "^2.2",
    "firebase/php-jwt": "^5.2",

    "mobiledetect/mobiledetectlib": "^2.8",
    "phpoffice/phpspreadsheet": "^1.17",
    "phpmailer/phpmailer": "^6.3",
    "nesbot/carbon": "^2.46.2",
    "dragonmantank/cron-expression": "^3.1",
    "zhuzhichao/ip-location-zh": "^2.4",
    "khanamiryan/qrcode-detector-decoder": "^1.0",
    "bacon/bacon-qr-code": "^2.0",
    "symfony/var-dumper": "^5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload" : {
    "psr-4" : {
      "Service\\": "Service/",
      "Domain\\": "Domain/",
      "Module\\": "Module/",
      "Infrastructure\\": "Infrastructure/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php artisan package:discover"
    ],
    "post-root-package-install": [
      "@php artisan gzo/composer/init"
    ]
  },
  "support" : {
    "email" : "zodream@fixmail.com"
  },
  "authors" : [ {
    "name" : "Jason Zou",
    "email" : "zodream@fixmail.com",
    "homepage" : "https://zodream.cn/",
    "role" : "Developer"
  } ]
}
