<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteCond %{HTTP_HOST} !^zodream.cn$ [NC]
RewriteRule ^(.*)$ http://zodream.cn/$1 [L,R=301]

RewriteCond %{REQUEST_FILENAME} !-f                             
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.php [L]                             
RewriteCond %{REQUEST_URI} !^.*(.css|.js|.ts|.gif|.png|.jpg|.jpeg)$


#RewriteCond %{HTTP_REFERER} !^http://zodream.cn/.*$ [NC]
#RewriteRule .*.(jpg|jpeg|gif|png|bmp|rar|zip|exe)$ http://zodream.cn/404.html [R,NC]

</IfModule>

