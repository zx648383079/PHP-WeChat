{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "typeRoots": [
      "./typings",
      "./node_modules/@types"
    ],
  },
  "exclude": [
    "bower_components",
    "node_modules",
    "vendor",
    "data"
  ]
}